{
  "kind": "collectionType",
  "collectionName": "tenants",
  "info": {
    "singularName": "tenant",
    "pluralName": "tenants",
    "displayName": "Tenant",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "tenantId": {
      "type": "uid",
      "targetField": "name"
    },
    "form": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::form.form",
      "mappedBy": "tenant"
    },
    "logo": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "isActive": {
      "type": "boolean",
      "default": true
    },
    "cloverMerchantId": {
      "type": "uid"
    },
    "openingTime": {
      "type": "time",
      "required": false,
      "default": "08:00"
    },
    "closingTime": {
      "type": "time",
      "default": "18:00"
    },
    "minutesInterval": {
      "type": "integer",
      "default": 60
    },
    "timeZone": {
      "type": "string",
      "default": "America/New_York",
      "required": true
    }
  }
}
