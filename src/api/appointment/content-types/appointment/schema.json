{
  "kind": "collectionType",
  "collectionName": "appointments",
  "info": {
    "singularName": "appointment",
    "pluralName": "appointments",
    "displayName": "Appointment",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string",
      "required": true
    },
    "email": {
      "type": "email",
      "required": true
    },
    "comment": {
      "type": "string"
    },
    "tenant": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::tenant.tenant"
    },
    "appointmentDay": {
      "type": "datetime",
      "required": true
    },
    "phone": {
      "type": "string",
      "required": false
    },
    "employee": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::employee.employee"
    },
    "service": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::service.service"
    }
  }
}
